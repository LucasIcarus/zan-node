{
    "project": {
        "projectName": "zan-template"
    },
    "hash": [
        {
            "src": "static/build/**/*.js",
            "type": "json",
            "dest": "./server/config/version_js.json",
            "recordTpl": "\"<%= name %>\" : \"<%= path %>.<%= extension %>\""
        },
        {
            "src": "static/build/**/*.css",
            "type": "json",
            "dest": "./server/config/version_css.json",
            "recordTpl": "\"<%= name %>\" : \"<%= path %>.<%= extension %>\""
        }
    ],
    "cdn": {
        "basic": {
            "src": [
                "static/build/**/*.js",
                "static/build/**/*.css"
            ],
            "dist": "static/build"
        },
        "img": {
            "src": [
                "client/www/img/**/*.*"
            ],
            "dist": "static/intro/img"
        }
    },
    "build": {
        "extra": {
            "shims": "",
            "paths": "",
            "commonModule": "",
            "appendEntry": true
        },
        "confPath": "",
        "js": {
            "src": "client",
            "local": "static/local",
            "build": "static/build"
        },
        "css": {
            "src": "client",
            "local": "static/local",
            "build": "static/build"
        }
    }
}